{
    "name": "IFrame Extractor",
    "version": "0.0.1",
    "manifest_version": 2,
    "browser_action": {
        "default_icon": {
            "19": "images/icon19.png",
            "38": "images/icon38.png"
            },
        "default_title": "IFrame Extractor",
        "default_popup": "yextract.html"
    },
    "background" : {
    "scripts": ["background.js"],
    "persistent": false
    },
    "content_scripts": [
      {
          "matches": ["http://*/*", "https://*/*"],
          "js": ["content.js"]
      }
    ],
    "permissions": [
      "activeTab"
  ]
}
